{
  "name": "aws-cdk-template",
  "version": "0.1.0",
  "bin": {
    "aws-cdk-template": "bin/aws-cdk-template.js"
  },
  "release": {
    "branches": [
      "main"
    ],
    "ci": true,
    "dryRun": false,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "npx eslint . --ext=.ts",
    "prettier": "npx prettier --check .",
    "test": "jest",
    "test-snapshot": "jest test/automation-k6-performance.snapshot.ts",
    "accept-snap": "jest test/automation-k6-performance.snapshot.ts -u",
    "mutation": "stryker run",
    "synth": "cdk synth",
    "deploy": "cdk deploy"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.98.0",
    "@stryker-mutator/core": "4.4.1",
    "@stryker-mutator/jest-runner": "4.4.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "26.0.10",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "7.20.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "prettier": "2.2.1",
    "jest": "26.4.2",
    "ts-jest": "26.5.3",
    "aws-cdk": "1.98.0",
    "ts-node": "9.0.0",
    "typescript": "4.3.5",
    "semantic-release": "17.4.4"
  },
  "dependencies": {
    "@aws-cdk/core": "1.98.0",
    "source-map-support": "^0.5.16"
  }
}
